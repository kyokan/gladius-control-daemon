{
  "name": "gladius-nm-beta",
  "scripts": {
    "start": "NODE_ENV=development npm-run-all -p js:watch style:watch",
    "js:watch": "webpack-dev-server",
    "js:build": "webpack",
    "build": "NODE_ENV=development npm-run-all -p js:build style:build",
    "style:build": "node-sass-chokidar src/ -o ../www/ --include-path src/styles",
    "style:watch": "npm run style:build && node-sass-chokidar src/ -o ../www/ --watch --recursive  --include-path src/styles"
  },
  "author": "Kyokan, LLC",
  "license": "UNLICENSED",
