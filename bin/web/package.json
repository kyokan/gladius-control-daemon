    "babel-cli": "6.26.0",
    "babel-env": "2.4.1",
    "babel-eslint": "^8.2.1",
    "babel-loader": "7.1.2",
    "babel-polyfill": "6.26.0",
    "babel-register": "6.26.0",
    "classnames": "2.2.5",
    "copy-webpack-plugin": "4.2.3",
    "html-webpack-plugin": "2.30.1",
    "lodash.get": "4.4.2",
    "lodash.pick": "4.4.0",
    "lodash.sortby": "^4.7.0",
    "lodash.uniqueid": "4.0.1",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "4.1.2",
    "prop-types": "15.6.0",
    "raf": "3.4.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.6",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "redux-actions": "2.2.1",
    "redux-thunk": "2.2.0",
    "rheostat": "^2.1.3",
    "svgo": "^1.0.4",
    "uglify-js": "3.2.1",
    "uglifyjs-webpack-plugin": "1.1.2",
    "web3": "1.0.0-beta.26",
    "webpack": "3.9.1",
    "webpack-dev-server": "2.9.5"
  }
